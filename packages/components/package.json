{
  "private": true,
  "name": "@codaholic/components",
  "version": "0.0.0",
  "description": "React components for CodeGlide",
  "main": "dist/index.js",
  "author": "Leko <leko.noor@gmail.com>",
  "repository": "Leko/codeglide",
  "license": "MIT",
  "scripts": {
    "prepare": "upbin npm-run-all prepare:clean prepare:babel prepare:types",
    "prepare:clean": "upbin rimraf dist",
    "prepare:babel": "upbin babel --config-file ../../.babelrc src --extensions '.ts,.tsx' --out-dir dist",
    "prepare:types": "upbin tsc --project tsconfig.build.json --declarationDir ./dist && cpx './dist/src/**/*.d.ts' ./dist && rm -rf ./dist/{src,__tests__}",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "echo 'FIXME: write test' && exit 0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0-alpha.24",
    "@storybook/addon-info": "^4.0.0-alpha.24",
    "@storybook/addon-knobs": "^4.0.0-alpha.24",
    "@storybook/addon-options": "^4.0.0-alpha.24",
    "@storybook/addon-viewport": "^4.0.0-alpha.24",
    "@storybook/react": "^4.0.0-alpha.24",
    "@types/lodash": "^4.14.117",
    "@types/react": "^16.4.15",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^3.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.0",
    "deindent": "^0.1.0",
    "language-map": "^1.4.0",
    "moment": "^2.22.2",
    "monaco-editor": "^0.14.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "upbin": "^0.8.1"
  },
  "dependencies": {
    "@codeglide/domain": "*",
    "@codeglide/languages": "*",
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "date-fns": "^1.29.0",
    "formik": "^1.3.1",
    "fuse.js": "^3.2.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-monaco-editor": "^0.19.0",
    "react-placeholder": "^3.0.1"
  }
}
